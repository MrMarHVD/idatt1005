<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.Region?>

<ToolBar prefHeight="51.0" prefWidth="851.0" xmlns="http://javafx.com/javafx/21" fx:controller="no.ntnu.idatt1005.plate.controller.toolbar.ToolbarController" xmlns:fx="http://javafx.com/fxml/1">
   <items>
      <Button fx:id="home" mnemonicParsing="false" onAction="#onHomeButtonPressed" prefHeight="50.0" prefWidth="100.0" text="Your Week" />
      <Button fx:id="cookbook" onAction="#onCookbookButtonPressed" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Cookbook" />
      <Button fx:id="inventory" onAction="#onInventoryButtonPressed" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Inventory" />
      <Button fx:id="shoppingList" onAction="#onShoppingListButtonPressed" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Shopping List" />
      <Region prefHeight="50.0" prefWidth="306.0" />
      <Button fx:id="settings" alignment="CENTER" contentDisplay="LEFT" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Settings" />
   </items>
</ToolBar>
